Request.php
	public function url($with_params = false, $only_domen = false) {
		$url = $this->server('HTTPS') == 'on' ? 'https://':'http://';
        $url.= $this->server('HTTP_HOST');

        if (!$only_domen) $url .= $this->server('REQUEST_URI');
        else $url .= '/';

		if (!$with_params) {
			$pos = strpos($url, '?');
			if ($pos !== false)
				$url = substr($url, 0, $pos);
		}
		return $url;
	}


